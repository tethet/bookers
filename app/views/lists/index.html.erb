<h1>Books</h1>

<% @lists.each do |list| %>
 <p>title</p>
 <span><%= link_to list.title, list_path(list.id) %></span>
<p>body</p>
 <span><%= link_to list.body, list_path(list) %></span>
<% end %>

<%= form_with model: @list do |f| %>

 <h2>New book</h2>
 <p>title</p>
  <%= f.text_field :title %>
 <p>body</p>
  <%= f.text_area :body %>

  <%= f.submit 'create book' %>

<% end %>
